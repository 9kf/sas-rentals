{
  "name": "sas-rentals",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android:dev": "EXPO_NO_DOTENV=1 expo run:android",
    "android": "APP_ENV=production expo run:android",
    "ios:dev": "EXPO_NO_DOTENV=1 expo run:ios",
    "ios": "APP_ENV=production expo run:android",
    "web": "expo start --web",
    "build:android": "eas build --profile production --platform android",
    "build:android:dev": "eas build --profile development --platform android",
    "build:android:preview": "eas build --profile preview --platform android",
    "eas-build-pre-install": "echo $GOOGLE_SERVICES_BASE64 | base64 --decode > ./android/app/google-services.json"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
